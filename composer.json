{
    "name": "mordilion/generated-abstract-hydrator",
    "description": "Extension-Library for Ocramius/GeneratedHydrator to use AbstractHydrator for the strategy functionality and recursive hydration",
    "keywords": ["generated hydrator", "recursive hydration"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Henning Huncke",
            "email": "mordilion@gmx.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mordilion\\GeneratedAbstractHydrator\\": "src/Mordilion/GeneratedAbstractHydrator",
            "Mordilion\\GeneratedAbstractHydrator\\Tests\\": "tests/Mordilion/GeneratedAbstractHydrator"
        }
    },
    "require": {
        "php": "~8.0.0 || ~8.1.0",
        "laminas/laminas-filter": "^2.11",
        "laminas/laminas-servicemanager": "^3.7",
        "ocramius/generated-hydrator": "^4.3.0"
    },
    "require-dev": {
        "vimeo/psalm": "^4.6",
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
